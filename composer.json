{
    "name": "aedart/athenaeum-support",
    "description": "Aware-of Helpers for Laravel components and other utilities",
    "keywords": [
        "Athenaeum",
        "Helpers",
        "Aware Of",
        "Properties",
        "Utils"
    ],
    "homepage": "https://aedart.github.io/athenaeum/",
    "support": {
        "issues": "https://github.com/aedart/athenaeum/issues",
        "source": "https://github.com/aedart/athenaeum"
    },
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "illuminate/config": "^7.22.4",
        "illuminate/support": "^7.22.4",
        "aedart/athenaeum-contracts": "^4.2.1",
        "aedart/athenaeum-utils": "^4.2.1"
    },
    "require-dev": {
        "twig/twig": "^2.13",
        "predis/predis": "~1.1",
        "symfony/console": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Support\\": "src"
        },
        "files": [
            "helpers/properties.php"
        ]
    },
    "bin": [
        "aware-of"
    ],
    "suggest": {
        "twig/twig": "Required for using aware-of generator"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "4.3.x-dev"
        }
    }
}
